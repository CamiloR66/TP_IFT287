			elementFormDefault="qualified">
			<!--mlns="urn:ift287:humanBody:element"-->
					
<xsd:element name="MainBody">
<xsd:complexType>
	<xsd:sequence>
		<xsd:element name="Systems" type="Systems"/>
		<xsd:element name="Organs" type="Organs"/>
	</xsd:sequence>
	<xsd:attribute name="bodyName" type="xsd:string"/>
    <xsd:attribute name="bodyID" type="xsd:integer"/>
</xsd:complexType>
  </xsd:element>

<xsd:complexType name="Systems">
	<xsd:sequence>
		<xsd:element maxOccurs="unbounded" name="System">
			<xsd:complexType>
              <xsd:sequence>
				<xsd:element maxOccurs="unbounded" name="Flow" type="Flow"/>
                </xsd:sequence>
				<xsd:attribute name="name" type="xsd:string" use="required"/>
				<xsd:attribute name="id" type="xsd:integer" use="required"/>
				<xsd:attribute name="type" type="xsd:integer" use="required"/>
			</xsd:complexType>
		</xsd:element>
	</xsd:sequence>
</xsd:complexType>

<xsd:complexType name="Flow">
	<xsd:sequence>
		<xsd:element maxOccurs="unbounded" name="Connectible" type="Connectible"/>
		<xsd:element maxOccurs="unbounded" name="Connections" type="Connections"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="id" type="xsd:integer" use="required"/>
</xsd:complexType>


